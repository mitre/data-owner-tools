{
  "version": 3,
  "clkConfig": {
    "l": 4096,
    "k": 30,
    "hash": {
      "type": "blakeHash"
    },
    "kdf": {
      "type": "HKDF",
      "hash": "SHA256",
      "keySize": 64
    }
  },
  "features": [
    {
      "identifier": "record_id",
      "ignored": true
    },
    {
      "identifier": "given_name",
      "format": {
        "type": "string",
        "encoding": "utf-8",
        "case": "mixed"
      },
      "hashing": {
        "strategy": {
          "bitsPerFeature": 300
        },
        "hash": {
          "type": "blakeHash"
        },
        "comparison": {
          "type": "ngram",
          "n": 2,
          "positional": false
        }
      }
    },
    {
      "identifier": "family_name",
      "format": {
        "type": "string",
        "encoding": "utf-8",
        "case": "mixed"
      },
      "hashing": {
        "strategy": {
          "bitsPerFeature": 300
        },
        "hash": {
          "type": "blakeHash"
        },
        "comparison": {
          "type": "ngram",
          "n": 2,
          "positional": false
        }
      }
    },
    {
      "identifier": "DOB",
      "format": {
        "type": "string",
        "encoding": "utf-8"
      },
      "hashing": {
        "strategy": {
          "bitsPerFeature": 300
        },
        "missingValue": {
          "sentinel": ""
        },
        "hash": {
          "type": "blakeHash"
        },
        "comparison": {
          "type": "ngram",
          "n": 2,
          "positional": false
        }
      }
    },
    {
      "identifier": "sex",
      "format": {
        "type": "enum",
        "values": [
          "A",
          "F",
          "M",
          "NI",
          "UN",
          "OT"
        ]
      },
      "hashing": {
        "strategy": {
          "bitsPerFeature": 200
        },
        "missingValue": {
          "sentinel": ""
        },
        "hash": {
          "type": "blakeHash"
        },
        "comparison": {
          "type": "ngram",
          "n": 1,
          "positional": false
        }
      }
    },
    {
      "identifier": "phone_number",
      "format": {
        "type": "string",
        "encoding": "utf-8"
      },
      "hashing": {
        "strategy": {
          "bitsPerFeature": 300
        },
        "hash": {
          "type": "blakeHash"
        },
        "comparison": {
          "type": "ngram",
          "n": 2,
          "positional": false
        }
      }
    },
    {
      "identifier": "household_street_address",
      "ignored": true
    },
    {
      "identifier": "household_zip",
      "ignored": true
    }
  ]
}
